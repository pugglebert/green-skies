<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="785.0" prefWidth="1369.0" style="-fx-background-color: #303030;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.main.AirportDataViewController">
    <children>
        <TableView fx:id="tableView" layoutX="173.0" layoutY="243.0" prefHeight="492.0" prefWidth="1184.0">
            <columns>
              <TableColumn fx:id="airportIDColumn" prefWidth="117.0" text="Airport ID" />
              <TableColumn fx:id="nameColumn" prefWidth="94.0" text="Airport Name" />
                <TableColumn fx:id="cityColumn" prefWidth="122.0" text="City" />
                <TableColumn fx:id="countryColumn" prefWidth="122.0" text="Coutry" />
                <TableColumn fx:id="IATAColumn" prefWidth="136.0" text="IATA" />
                <TableColumn fx:id="ICAOColumn" prefWidth="153.0" text="ICAO" />
                <TableColumn fx:id="latitudeColumn" minWidth="0.0" prefWidth="164.0" text="Latitude" />
                <TableColumn fx:id="longitudeColumn" minWidth="0.0" prefWidth="116.0" text="Longitude" />
                <TableColumn fx:id="altitudeColumn" minWidth="0.0" prefWidth="150.0" text="Altitude" />
                <TableColumn fx:id="timezoneColumn" minWidth="0.0" prefWidth="99.0" text="Timezone" />
                <TableColumn fx:id="DSTColumn" minWidth="0.0" prefWidth="99.0" text="DST" />
                <TableColumn fx:id="dataBaseTimeZoneColumn" minWidth="0.0" prefWidth="99.0" text="Database Time Zone" />
            </columns>
        </TableView>
        <VBox prefHeight="785.0" prefWidth="160.0" style="-fx-background-color: #007f5f;">
            <children>
                <Button fx:id="btnUpload" alignment="CENTER" mnemonicParsing="false" onAction="#toUploadData" onMouseClicked="#toUploadData" prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="Upload Data" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="18.0" />
                    </font>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="btnRouteDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toRouteDataView" prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Routes" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="18.0" />
                    </font>
                </Button>
                <Button fx:id="btnAirportDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toAirportDataView" prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Airports" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="18.0" />
                    </font>
                </Button>
                <Button fx:id="btnAirlineDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toAirlineDataView" prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Airlines" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="18.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>


