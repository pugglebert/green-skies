<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="800.0" prefWidth="1400.0" style="-fx-background-color: #303030;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.main.FlightDataViewController">
   <children>
      <VBox prefHeight="785.0" prefWidth="160.0" style="-fx-background-color: #007f5f;">
         <children>
            <Button fx:id="btnUpload" alignment="CENTER" mnemonicParsing="false" onAction="#toUploadData"
                    onMouseClicked="#toUploadData" prefHeight="72.0" prefWidth="220.0"
                    style="-fx-background-color: transparent;" text="Upload Data" textFill="WHITE">
               <font>
                  <Font name="Arial" size="18.0"/>
               </font>
               <VBox.margin>
                  <Insets top="20.0"/>
               </VBox.margin>
            </Button>
            <Button fx:id="btnRouteDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toRouteDataView"
                    prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Routes"
                    textFill="WHITE">
               <font>
                  <Font name="Arial" size="18.0"/>
               </font>
            </Button>
            <Button fx:id="btnAirportDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toAirportDataView"
                    prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Airports"
                    textFill="WHITE">
               <font>
                  <Font name="Arial" size="18.0"/>
               </font>
            </Button>
            <Button fx:id="btnAirlineDataView" alignment="CENTER" mnemonicParsing="false" onAction="#toAirlineDataView"
                    prefHeight="72.0" prefWidth="220.0" style="-fx-background-color: transparent;" text="View Airlines"
                    textFill="WHITE">
               <font>
                  <Font name="Arial" size="18.0"/>
               </font>
            </Button>
         </children>
      </VBox>
      <TableView fx:id="FlightTable" layoutX="312.0" layoutY="222.0" prefHeight="200.0" prefWidth="200.0">
         <columns>
            <TableColumn prefWidth="75.0" text="C1"/>
            <TableColumn prefWidth="75.0" text="C2"/>
         </columns>
      </TableView>
   </children>
</AnchorPane>
