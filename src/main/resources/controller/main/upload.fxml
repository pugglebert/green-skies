                <ButtonType fx:constant="OK" />
            </buttonTypes>
        </DialogPane>
      <ChoiceBox fx:id="dataTypeSelect" layoutX="411.0" layoutY="317.0" prefHeight="42.0" prefWidth="252.0" />
      <ListView fx:id="fileView" layoutX="1024.0" layoutY="173.0" prefHeight="510.0" prefWidth="434.0" />
      <Button fx:id="backButton" layoutX="291.0" layoutY="691.0" mnemonicParsing="false" onAction="#backToWelcome" prefHeight="42.0" prefWidth="120.0" text="Back" />
      <Button fx:id="browseButton1" alignment="CENTER_RIGHT" layoutX="708.0" layoutY="317.0" mnemonicParsing="false" onAction="#browseFiles" prefHeight="42.0" prefWidth="120.0" text="Browse files " textAlignment="CENTER" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Text fx:id="fileAcceptedText" layoutX="385.0" layoutY="224.0" strokeType="OUTSIDE" strokeWidth="0.0" visible="false" wrappingWidth="434.0" />
      <Button fx:id="addSingle" layoutX="708.0" layoutY="400.0" mnemonicParsing="false" onAction="#addSingle" prefHeight="42.0" prefWidth="120.0" text="Add single entry" />
