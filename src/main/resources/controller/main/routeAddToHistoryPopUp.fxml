<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="700.0" prefWidth="1400" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.main.RouteAddToHistoryPopUpController">
    <children>
        <TableView fx:id="tableView" layoutX="124.0" layoutY="130.0" prefHeight="441.0" prefWidth="1262.0">
            <columns>
                <TableColumn fx:id="passengerNumber" prefWidth="110" text="No. of passenger"/>
                <TableColumn fx:id="airlineNameColumn" prefWidth="117.0" text="Airline Name"/>
                <TableColumn fx:id="sourceAirportColumn" prefWidth="122.0" text="Source Airport"/>
                <TableColumn fx:id="destinationAirportColumn" prefWidth="153.0" text="Destination Airport"/>
                <TableColumn fx:id="codeShareColumn" minWidth="0.0" prefWidth="116.0" text="Code Share"/>
                <TableColumn fx:id="numOfStopsColumn" minWidth="0.0" prefWidth="131.0" text="Number of Stops"/>
                <TableColumn fx:id="equipmentColumn" minWidth="0.0" prefWidth="94.0" text="Equipment"/>
            </columns>
        </TableView>
        <Button fx:id="confirmBtn" layoutX="954.0" layoutY="612.0" mnemonicParsing="false" onAction="#confirm"
                prefHeight="31.0" prefWidth="93.0" text="Confirm"/>
        <Button fx:id="cancelBtn" layoutX="1072.0" layoutY="612.0" mnemonicParsing="false" onAction="#cancel"
                prefHeight="31.0" prefWidth="69.0" text="Cancel"/>
        <Button fx:id="setAllPassengerBtn" layoutX="124.0" layoutY="581.0" mnemonicParsing="false"
                onAction="#setAllPassengerTo1" prefHeight="31.0" prefWidth="122.0" text="Set All Passenger 1"/>
        <Text layoutX="441.0" layoutY="89.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Tips: Double click on and press enter after changing number of passenger in order to save the value">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
